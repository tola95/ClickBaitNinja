{	
  "manifest_version": 2,
  "name": "ClickBait Ninja",
  "version": "1.0",

  "description": "An app that uses natural language processing to answer clickbait",
  "icons": {
    "128": "icon128.png"
  },

  "permissions": ["declarativeContent", "storage"],

  "background": {
    "scripts": ["my_file.js"],
    "persistent": false
  },

  "browser_action": {
    "default_popup": "popup.html"
  }
}